{
  "name": "epoxyjs",
  "version": "0.5.2",
  "description": "Proxy-based reactive library for Javascript and Typescript",
  "main": "./dist/epoxy.js",
  "types": "./dist/epoxy.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeatonTech/epoxy_js"
  },
  "scripts": {
    "build": "rm -r ./dist; tsc",
