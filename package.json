  "dependencies": {
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "npm": "^5.10.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  }
}
